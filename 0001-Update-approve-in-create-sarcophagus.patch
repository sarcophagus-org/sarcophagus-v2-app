+      //   dispatch(setArchaeologistOnlineStatus(peerId.toString(), false));
+      // }, pingThreshold);
+    },
+    [dispatch]
+  );
+
+  const onPeerDisconnect = useCallback(
+    (evt: CustomEvent<Connection>) => {
+      const peerId = evt.detail.remotePeer.toString();
