+          align="left"
+          pl={4}
+          mt={3}
+          spacing={5}
+        >
+          {archaeologists.map(arch => (
+            <ArchaeologistDetailItem
+              key={arch.address}
+              archaeologist={arch}
+              sarcoHasRewraps={!!sarcoHasRewraps}
+              isResurrected={sarcophagus.publishedPrivateKeyCount >= sarcophagus.threshold}
+            />
+          ))}
+        </VStack>
+      </Collapse>
+    </>
+  );
+}
